.yuyuebanner{width: 100%;max-width: 640px; padding-bottom: 0.25rem;}
.yuyue_login{margin:0 3.75%; overflow:hidden;}
.yuyue_login .xiang{height:2.5rem; overflow:hidden; margin-top: 0.625rem;position: relative;}
.yuyue_login .xiang .inputk{-webkit-appearance: none;border-radius: 0;background: url(../images/qsrmm.png) no-repeat 0.625rem center;background-size: 1.25rem auto; padding: 0 13% 0 0; height:2.3rem; display:inline-block;padding-left: 2.44rem;border: 1px #dcdcdc solid;width: 100%; line-height:2.44rem; font-size:0.8rem; outline:none; color:#716f6f;}
.yuyue_login .xiang .inputk.ys1{background: url(../images/qsrkh.png) no-repeat 0.625rem center;background-size: 1.25rem auto;}
.yuyue_login .xiang .delete1{position: absolute;right: 0.5rem;bottom: 0.85rem; background:url(../images/close.png) right center no-repeat; background-size:0.9rem auto; width: 1rem;height: 1rem;display: inline-block;}
.yuyuebanner img{width: 100%;}

.yuyue_login_tips{padding:0.75rem; clear:both; overflow:hidden; font-size:0.65rem; line-height:1.15rem; color:#999999;}
.yuyue_login_tips h2{font-size:0.8rem; font-weight:normal; color:#716f6f;margin-bottom: 0.5rem;}

.reg_an3{clear:both; overflow:hidden; padding:0 0.75rem;margin-top: 0.5rem;}
.reg_an3 a{height:2rem; background:#e25d39; display:block; text-align:center; font-size:0.8rem; color:#fff; line-height:2rem; border-radius:3px;}