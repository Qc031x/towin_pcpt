@charset "utf-8";
/*header*/
.header .header_midd{width:60%;border:1px solid #027ac8;height:1.45rem;line-height:1.45rem;border-radius:1.25rem;margin:0.5rem 0;overflow: hidden;background:#fff;color:#027ac8;}
.header .header_midd span{display:inline-block; float: left; width:50%;height:1.7rem;text-align:center;background:#fff;color:#027ac8;}
.header .header_midd span a{color:#027ac8;font-size:0.7rem;}
.header .header_midd span.activeR{width:50%;background:#027ac8;color:#fff;}
.header .header_midd span.activeR a{color:#fff}
.header .header_right{width:20%;overflow:hidden;}


/*nav*/
.nav{width:100%;max-width:640px;height:2.2rem;line-height: 2.2rem;font-size:0.6rem;position:fixed;z-index: 111;background:#fff;}
.navcon{overflow:hidden;}
.navcon li{width:25%;height: 2.2rem;;float:left;box-sizing:border-box;border:1px solid #eee;border-right:0;border-top:0;/*padding:0 0.25rem 0 0.35rem;text-align:center;*/position:relative;}
.navcon li a{display:inline-block;width:100%;text-align:center;}
.navcon li img{width:0.5rem; height:0.3rem;position:absolute;top:22px;right:5px;transform:rotate(0deg);transition:transform linear 0.2s;}
.navcon li.active img{transform:rotate(180deg);transition:transform linear 0.2s;}
.navcon li.active a{color:#027ac8;}

/*prolist*/
.prolist{overflow: hidden;margin:9px 6px;}
.prolist .list{ overflow: hidden; border: 1px solid #dbdbdb; margin: 0 1% 10px 0; display: block; float: left; width: 49.5%;}
.prolist .list .imgbox{ margin: 10px 6px; position: relative; width: 6.9rem; height: 4.35rem;}
.prolist .list .imgbox span{width:30px;text-align:center;display:inline-block;height:12px;line-height:12px;position:absolute;color:#fff;top:1px;font-size:9px;}
.prolist .list .imgbox span.list_cu{background:#ff8f00;left:0;}
.prolist .list .imgbox span.list_zhe{background:#ff0000;right:1px;}
.prolist .list .imgbox span.list_zhe em{font-style: normal;}
.prolist .list img{ width: 6.9rem; height: 4.35rem;}
.prolist .imgleft{position: absolute; width: 40px; font-size: 9px; line-height: 12px; padding: 0.15rem 0; top: 0; left: 6%; color: #fff; text-align: center;}
.prolist .imgleft1{position: absolute; width: 60px; height: 30px; top: 0; left: 10px;}
.prolist .imgleft1 img{width: 67%; height: auto; border-radius: 0;}
.prolist .imgright{position: absolute; width: 54px; font-size: 8px; height: 43px; background: url("../images/tjsal.png") no-repeat right top; background-size: 67% auto; top: 0; right: 0; color: #fff;text-align: right; padding: 3px 1px 0 0;}

.prolist .recommend{position: absolute; top: 40px; left: 10px; color: #fff; font-size: 9px;}
.prolist .recommend p{margin: 0;}
.prolist .recommend .rectitle{ font-size: 11px; font-weight: bold;}

.prolist .list .info{overflow: hidden; margin: 0 6px;}
.prolist .list .info .info_col{ color: #6e6e6f; font-size: 10px;margin:2px 0;}
.prolist .list .info .info_bor{margin:3px 0;}
.prolist .list .info .titlebox{ font-size: 12px; color: #666666; font-weight: bold; line-height: 17px;margin:0;height: 17px; overflow: hidden;}


.prolist .list p{ overflow: hidden; width: 100%;}
.prolist .list .nowpla{display: block; float: left; line-height: 20px; color: #ff0000; font-size: 12px; font-weight: bold; padding: 0 0 0 9px; background: url("../images/money.png") no-repeat left 5px; background-size: 7px auto;}
.prolist .list .ordered{line-height: 20px; color: #999; font-size: 10px; display: block; float: right;}
.prolist .list:hover{ border: 1px solid #027ac8; }
.prolist .list:nth-child(2n){ margin: 0;}


