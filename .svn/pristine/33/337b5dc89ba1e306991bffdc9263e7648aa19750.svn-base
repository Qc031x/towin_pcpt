@charset "utf-8";
body {
	background: #e5e5e5;
}

nav {
	width: 100%;
	height: 4rem;
	background: #fff;
	position: fixed;
	top: 0;
	z-index: 99999999;
	max-width: 640px;
	margin: 0 auto;
	border-bottom: 2px solid #e8e8e8;
}

.nav-left {
	float: left;
	font-size: 1.6rem;
	color: #666;
	padding: 0 1.56% 0 3.125%;
	line-height: 3;
}

.nav-left span {
	display: block;
	float: left;
	margin-left: 0.5rem;
}

.nav-left img {
	width: 18%;
	padding-top: 1.4rem;
	float: left;
}

.nav-mid {
	float: left;
	width: 46.785%;
	font-size: 1.6rem;
	color: #666;
	line-height: 2.5;
	padding-top: 2.2%;
	margin-left: 5%
}

.nav-mid p {
	text-align: center;
	color: #333;
}

.nav-mid ul {
	width: 100%;
	border: 1px solid #027ac8;
	border-radius: 24px;
}

.nav-mid ul li {
	float: left;
	width: 50%;
	text-align: center;
	color: #027ac8;
	height: 2.5rem;
	line-height: 2.5rem;
}

.nav-mid ul li:first-child {
	border-right: 1px solid #027ac8;
}

.nav-mid ul li.myActive {
	color: #fff;
	background: #027ac8;
}

.nav-right {
	float: right;
	font-size: 1.6rem;
	color: #666;
	line-height: 2.8;
	padding: 0 1.56% 0 3.125%;
}

.nav-right img {
	width: 30%;
}

#model_box2 {
	width: 100%;
	height: 100%;
	z-index: 999999;
	position: fixed;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	overflow: auto;
	background-color: rgba(0, 0, 0, 0.5);
	max-width: 640px;
	margin: 0 auto;
	min-width: 320px;
	display: none;
}

#model_box2 .indexNav {
	width: 100%;
	margin-top: 4rem;
}

#model_box2 .indexNav ul.list {
	background: #fff;
	width: 100%;
	max-width: 640px;
	margin: 0 auto;
}

#model_box2 .indexNav ul.list li {
	float: left;
	width: 25%;
	border: 1px solid #e5e5e5;
	text-align: center;
	padding: 1.5rem 0;
}

#model_box2 .indexNav ul.list li p {
	margin: 1.5% 0;
	font-size: 1.4rem;
}

#model_box2 .indexNav ul.list li img {
	width: 35%;
}

.page {
	margin-top: 4rem;
}