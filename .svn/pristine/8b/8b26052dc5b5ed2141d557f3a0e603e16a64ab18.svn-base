@media only screen and (min-width: 414px) {html {font-size: 25.875px !important}}
@media only screen and (min-width: 480px) {html {font-size: 30px !important}}
@media only screen and (max-width: 320px){html{font-size: 20px;} } */

* { padding: 0; margin: 0; list-style: none; -webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box ;-webkit-tap-highlight-color:rgba(255,255,255,0);}
html { height: 100%; -webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box; }
body { width: 100%; height: 100%;max-width: 640px; font-family: Arial, "Microsoft Yahei"; font-size: 14px; margin:auto; padding: 0; position: relative; background: #fff; color: #3c3c3c }
a { color: #3c3c3c; text-decoration:none; }
a:link { color: #3c3c3c }
img { width: 100%; outline: 0; border: 0 }
ul { padding: 0; margin: 0; list-style: none }
b{font-weight:normal;}
dd, dt { padding: 0; margin: 0 }
input { box-shadow: none; -webkit-box-shadow: none; outline:none;font-family: Arial, "Microsoft Yahei";-webkit-appearance: none; -moz-appearance: none; -o-appearance: none; appearance: none;}
select{ font-family: Arial, "Microsoft Yahei"; box-shadow: none; -webkit-box-shadow: none;-webkit-appearance: none; -moz-appearance: none; -o-appearance: none; appearance: none;}
i { font-style: normal; font-family: Arial, "Microsoft Yahei";}
textarea{font-family: Arial, "Microsoft Yahei"; box-shadow: none; -webkit-box-shadow: none;-webkit-appearance: none; -moz-appearance: none; -o-appearance: none; appearance: none;}
.nowrap{text-overflow: ellipsis;overflow: hidden;white-space: nowrap;}
.main { width: 100%; min-width: 320px; max-width: 640px; position: relative; margin: auto;}
.topblank{height: 2.5rem; width: 100%; background: #fff;}
.bottomblank{height: 3rem; width: 100%; background: #eeeff3;}
.bottomblank1{height: 3rem; width: 100%; background: #fff;}
.bottomblank2{height: 2rem; width: 100%; background: #fff;}
.bottomblank3{height: 100px; width: 100%; background: #eeeff3;}
.clear{clear: both;}
em{ font-style: normal;}

/*分界*/
.interval{height: 5px; width: 100%; background: #eeeff3;}
.interval1{height: 10px; width: 100%; background: #eeeff3;}
.interval2{height: 23px; width: 100%; background: #eeeff3;}


/*页面头部**/
body{background:#e8e8e8}
.header{width:100%;max-width:640px;height:2.5rem;line-height:2.5rem;overflow:hidden;background:#fff;font-size:0.85rem;position:fixed;z-index:1000;border-bottom:1px solid #e8e8e8;}
.header div{float:left;}
.header .header_left{width:20%;}
.header .header_left a{color:#666;display:inline-block;width:89%;padding-left:0.775rem;font-size:0.7rem;margin-left:0.5rem;background:url(../images/left.png) no-repeat left 0.8rem;background-size:0.4rem auto;}
/* .header .header_midd{width:58%;border:2px solid #fff;height:1.85rem;line-height:1.85rem;border-radius:1.25rem;margin:0.3rem 0;overflow: hidden;background:#fff;color:#027ac8;}
.header .header_midd span{display:inline-block;width:48%;height:1.65rem;text-align:center;background:#1aa9b9;color:#fff;}
.header .header_midd span.activeR{float:right;width:51%;background:#027ac8;color:#fff;} */
.header .header_right{width:20%;overflow:hidden;}
.header .header_right span{color:#666;font-size:0.7rem;}
.header .header_right .header_rbg{width:1.05rem;height:0.75rem;background:url(../images/list.png) no-repeat center center;background-size:0.75rem 0.55rem;margin-top:0.86rem;margin-left:0.5rem}
/*.header .header_right .header_rbg.rbgchange{background:url(../images/menu_bg.png) no-repeat center center;background-size:14px 14px;}*/
.header .header_info{width:58%;height:1.85rem;line-height:1.85rem;/* color:#fff; */margin:0.3rem 0; text-align: center;}


/*menu点击弹框*/
.linklist{width:490px;position:absolute;top:40px;right:0;padding-top: 1rem;  display: none;z-index: 8888;}
.linklist li{width:100%;background:rgb(26,169,185);height:40px;line-height:40px;padding-left:13px;position:relative;}
.linklist li a{display: inline-block;  width: 100%;;height:25px;line-height:25px;padding-left:23px;color:#fff;font-size:12px;}
.linklist li:first-child:before{content: " ";position: absolute;bottom: 100%;right:19px;border-left: 0.25rem solid transparent;border-right: 0.25rem solid transparent;  border-bottom: 0.5rem solid rgb(26,169,185);  }
.linklist li:first-child a{background:url(../images/menu_h.png)no-repeat left center;background-size:15px 15px;}
.linklist li:nth-child(2) a{background:url(../images/menu_u.png)no-repeat left center;background-size:15px 15px;}
.linklist li:last-child a{background:url(../images/menu_sear.png)no-repeat left center;background-size:15px 15px;}
ul.list {
	position: fixed;
	top: 2.4rem;
	z-index: 999999999;
	background: #fff;
	display: none;
	border-top:1px solid #e5e5e5; 
	width:100%;
	max-width:640px;
	margin:0 auto;
}

ul.list li {
	float: left;
	width: 25%;
	border-right: 1px solid #e5e5e5;
	border-bottom: 1px solid #e5e5e5;
	text-align: center;
	padding: 0.4rem 0;
}

ul.list li p {
	margin:0;
	font-size: 0.5rem;
}

ul.list li img {
	width: 30%;
}


/*底部*/
.bottom {float:left;width:100%; margin:0; text-align:center; border-top:#027ac8 3px solid; }
.bottom .footer { float: left; width: 100%; height: auto; margin: 0; padding:25px 0 0; }
.bottom .footer p{ float:left; width:100%; line-height:20px; font-size:10px; color:#aaaaaa; margin:0 0 10px;}
.bottom .footer p.exp{ margin:10px 0 4px; font-size:12px;}
.bottom .footer p a{ display:inline-block; font-size:10px; color:#484848; padding:0 10px;}
.bottom .footer p a.footer_login{ line-height:30px; padding-left:28px; font-size:11px; color:#3d4245; background:url(../images/footer_login.png) 10px center no-repeat; background-size:12px; border:#CCC 1px solid; border-radius:4px; margin:0 10px;}
.bottom .footer p a.footer_reg{ line-height:30px; padding-left:28px; font-size:11px; color:#3d4245; background:url(../images/footer_reg.png) 10px center no-repeat; background-size:12px; border:#CCC 1px solid; border-radius:4px; margin:0 10px;}
.bottom .footer p a.footer_cus{ line-height:30px; padding-left:28px; font-size:11px; color:#3d4245; background:url(../images/footer_cus.png) 10px center no-repeat; background-size:12px; border:#CCC 1px solid; border-radius:4px; margin:0 5px;}
.bottom .footer p a.footer_loginOut{ line-height:30px; padding-left:28px; font-size:11px; color:#3d4245; background:url(../images/footer_quit.png) 10px center no-repeat; background-size:12px; border:#CCC 1px solid; border-radius:4px; margin:0 10px;}

/*服务保障*/
.sergua{ float:left; width:100%; height: auto; padding:20px 0 15px 0;}
.sergua li{ float:left; width:23%; height: auto; margin:0 1%;}
.sergua li i{ float:left; width:100%; text-align:center; margin-bottom:3px;}
.sergua li i img{ width:32px;}
.sergua li em{ float:left; width:100%; text-align:center; font-style:normal; font-size:13px; color:#4b4643;}
.sergua li p{ float:left; width:100%; height:auto; line-height:15px; text-align:center; font-size:10px; color:#999999; margin:0;}

/*购买按钮*/
.buybox{ position:fixed;z-index: 9997; bottom:0; width:100%; max-width:640px; min-width:320px; height:3rem; line-height:3rem; border-top:#ddd 1px solid;background:#fff;}
.buybox a{ position:absolute;top:0;width:93.6%; height:2rem;line-height:2rem; text-align:center; text-decoration:none; font-size:18px; font-weight:normal; color:#656565; background-color:#FFF;margin:0.5rem 3.2%;border-radius:5px;}
.buybox a.buybtn{ background-color:#e25d39; color:#FFF;}

/*加载更多按钮*/
.loadMore{ height: 2.2rem;clear: both;overflow: hidden;line-height:2.2rem;text-align: center;z-index: 1;}
.loadMore span{height: 2.2rem;display: inline-block;padding: 0 1.15rem 0 0;background: url(../images/gengduo.png) right center no-repeat;background-size: 0.75rem auto;font-size: 0.65rem;color: #027ac8;}



#model_box1 {
	width: 100%;
	height: 100%;
	z-index: 999;
	position: fixed;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	overflow: auto;
	background-color: rgba(0, 0, 0, 0.5);
	max-width: 640px;
	margin: 0 auto;
	min-width: 320px;
	display: none;
}

#model_box1 .indexNav {
	width: 100%;
	margin-top: 8rem;
}

