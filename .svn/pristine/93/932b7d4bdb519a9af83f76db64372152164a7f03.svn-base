@charset "utf-8";
/*header*/
.header .header_midd{width:60%;border:1px solid #027ac8;height:1.45rem;line-height:1.45rem;border-radius:1.25rem;margin:0.5rem 0;overflow: hidden;background:#fff;color:#027ac8;}
.header .header_midd span{display:inline-block; float: left; width:50%;height:1.7rem;text-align:center;background:#fff;color:#027ac8;}
.header .header_midd span a{color:#027ac8;font-size:0.7rem;}
.header .header_midd span.activeR{width:50%;background:#027ac8;color:#fff;}
.header .header_midd span.activeR a{color:#fff}
.header .header_right{width:20%;overflow:hidden;}


/*menu*/
.linklist{width:90px;position:absolute;top:40px;right:0;padding-top: 1rem;  display: none;z-index: 8888;}
.linklist li{width:100%;background:rgb(26,169,185);height:40px;line-height:40px;float:left;padding-left:13px;position:relative;}
.linklist li a{display: inline-block;  width: 100%;;height:25px;line-height:25px;padding-left:23px;color:#fff;font-size:12px;}
.linklist li:first-child:before{content: " ";position: absolute;bottom: 100%;right:19px;border-left: 0.25rem solid transparent;border-right: 0.25rem solid transparent;  border-bottom: 0.5rem solid rgb(26,169,185);  }
.linklist li:first-child a{background:url(../images/menu_h.png)no-repeat left center;background-size:15px 15px;}
.linklist li:nth-child(2) a{background:url(../images/menu_u.png)no-repeat left center;background-size:15px 15px;}
.linklist li:last-child a{background:url(../images/menu_sear.png)no-repeat left center;background-size:15px 15px;}

.nav{width:100%;max-width:640px;height:1.8rem;line-height: 1.8rem;font-size:0.6rem;position:fixed;z-index: 111;background:#fff;}
.navcon{overflow:hidden;}
.navcon li{width:25%;height: 1.8rem;;float:left;box-sizing:border-box;border:1px solid #eee;border-right:0;border-top:0;/*padding:0 0.25rem 0 0.35rem;text-align:center;*/position:relative;}
.navcon li a{display:inline-block;width:100%;text-align:center;}
.navcon li img{width:0.5rem; height:0.3rem;position:absolute;top:18px;right:5px;transform:rotate(0deg);transition:transform linear 0.2s;}
.navcon li.active img{transform:rotate(180deg);transition:transform linear 0.2s;}
.navcon li.active a{color:#027ac8;}


/*区域*/
.filter-container{position:fixed;z-index:998;top:4.3rem;width:100%;min-width:320px;max-width:640px;height:auto;max-height:100%;overflow:auto;background: rgba(255,255,255,0.98);}
.filter-container>div{height: auto;position: relative;overflow: hidden;}
.filter-container .area_box_qc{display:none;}
.filter-container .area_box_yiyuan{display:none;}
.filter-container .area_box_paixu{display:none;}
.area_box{width: 100%;float: left;max-height: 30rem;overflow: hidden;}
.area_box .area_box_list{width: 100%;  max-height: 30rem;  overflow-y: auto;-webkit-overflow-scrolling: touch;float:left;}
.area_box .area_box_list ul{float: left;  width: 100%;}
.area_box .area_box_list ul li{ float: left;width:100%;border-bottom: 1px solid #dcdcdc; /* border-right: 1px solid #dcdcdc; */}
.area_box .area_box_list .current{color:#027ac8;background: #e5e5e5;  border-right: 0;}
.area_box .area_box_list .active{color:#027ac8;background: #e5e5e5;  border-right: 0;}
.area_box .area_box_list ul li a{display: block;padding:15px 10px; font-size:12px;text-decoration: none;}
.area_box .area_box_list .current a{color: #027ac8;}
.area_box .area_box_list .active a{color: #027ac8;}
.filter-cont_mask{width:100%;max-width: 640px;height:100%;position:fixed;background:#404040;opacity:0.4;z-index:100;display:none;}


/*位置*/
.weizhi{width:100%;font-size:0.6rem;background:#eee;height:1.8rem;line-height:1.8rem;padding:0 0.5rem;}
.weizhi .wz_bread{width:100%;background:url(../images/bre_nav_ref.png)no-repeat right center;background-size:0.85rem auto;color:#adadad}
.weizhi .wz_bread a{color:#adadad}


/**/
.jigou_list{width:100%;background:#fff;}
.jigou_list_con{width:100%;overflow:hidden;padding:0.75rem 0.5rem;position:relative;border-bottom:1px solid #eee;}
.jigou_list_con>div{float:left;}
.jigou_list_con .jigou_img{width:4.8rem;height:3.0rem;overflow:hidden;}
.jigou_list_con .jigou_img img{width:100%;float:left;height:100%;}
.jigou_list_con .jigou_intro{margin-left:0.5rem;font-size:0.6rem;height:3.0rem;position: absolute;top: 15px;left: 5.0rem;right: 0;width: auto;line-height: 20px;overflow: hidden;}
.jigou_list_con .jigou_intro div{line-height:0.9rem;margin-bottom:0.2rem;font-size:0.5rem;}
.jigou_list_con .jigou_intro div:last-child{margin-bottom:0;}
.jigou_list_con .jigou_intro .jg_inro1{font-weight6700;margin-left:-0.3rem; color: #6e6e6f;width:10.8rem;height:0.9rem;overflow:hidden;font-size:0.6rem;}
.jigou_list_con .jigou_intro .jg_inro3 span:first-child{display:inline-block;width:40%;padding-left:0.575rem;background:url(../images/dw_icon.png)no-repeat left center;background-size: 0.45rem auto;}
.jigou_list_con .jigou_intro .jg_inro3 span:first-child em{color:#9d9d9d;margin-left:3px;font-style:normal;}
.jigou_list_con .jigou_intro .jg_inro3 span:nth-child(2){display:inline-block;color:#9d9d9d;text-align:center;}
.jigou_list_con .jigou_intro .jg_inro3 span:last-child{display:inline-block;width:37%;text-align:right;color:#9d9d9d;position:absolute;right:10px;}

.fixed{position:fixed;z-index: 9999;top:2.2rem;left:0;}


















