/**/
.tjchead{overflow: hidden; border-bottom: 1px solid #ddd; margin: 0 0.8rem;}
.imgbox{ padding: 0.6rem 0 1.2rem; position: relative;}
.imgbox img{ border-radius: 0.625rem; width: 100%;}
.imgbox .imgleft{ position: absolute; top: 0.6rem; left: 6.5%; width: 3.8rem; height: 2rem; padding: 0.25rem; line-height: 18px; /*line-height: 0.8rem;*/ text-align: center; font-size: 0.75rem; color: #fff;}
.imgbox .imgright{ position: absolute; bottom: 2rem; left: 0; width: 5rem; height: 1.8rem; line-height: 1.8rem; text-align: center;  background: url("../images/price.png") no-repeat; background-size: 5rem auto; font-size: 0.75rem; color: #fff;}
.imgbox .recommend{ position: absolute; height: 5rem; top: 1rem; bottom: 0; margin: auto; left: 6.5%; color: #fff; font-size: 0.6rem;}
.imgbox .recommend .rectitle{ font-size: 1.05rem; font-weight: bold; margin: 0 0 0.25rem;}
.imgbox .recommend .paddleft{ line-height: 0.9rem; background: url("../images/circle.png") no-repeat left center; background-size: 0.6rem auto; padding: 0 0 0 0.8rem;}

.info{overflow: hidden; margin: 0 0.8rem 2.0rem;}
.info p{line-height: 2.175rem; height: 2.175rem; overflow: hidden; border-bottom: 1px solid #ddd; font-size: 0.65rem; color: #373433;}
.info p .ocol{color: #e25d39;}

.buybox1{width:100%; max-width:640px; min-width:320px; height:3rem; line-height:3rem;}
.buybox1 a{display: block; width:93.6%; height:2rem;line-height:2rem; text-align:center; text-decoration:none; font-size:18px; font-weight:normal; color:#FFF; background-color:#e25d39;margin:0 3.2%;border-radius:5px;}

.alertBg{width: 100%; height: 100%; position: fixed; top: 0; left: 0; z-index: 9999; background: url(../images/hstmbg.png) 0 0 repeat; display: none;}
.alertBox{width: 10.3rem; height: 12.575rem; background: #fff; overflow: hidden; position: fixed; top: 50%; left: 50%; margin: -7rem 0 0 -5rem; border-radius: 5px; z-index: 99999; display: none;}
.alertBox img{width: 100%; display: block;}

.alertBox .alertCon{margin: 0.65rem 0 0.9rem 1.5rem; font-size: 0.8rem; color: #6c6c6c;}
.alertBox .alertCon p{line-height: 1.5rem; height: 1.5rem; overflow: hidden;}
.alertBox a{display: block; background: #e25d39; width: 100%; height: 2rem; color: #fff; text-align: center; line-height: 2rem;}